        <script>
          function showEditModal(id,id_barang, filename, nama_barang, gram, tgl_input, tgl_update) {
            $('#updatemodal').modal('show')
            document.getElementById('mdl_update_id').value = id
            document.getElementById('upd_id_barang').value = id_barang
            document.getElementById('upd_nama_barang').value = nama_barang
           // document.getElementById('upd_merk_barang').value = merk 
            document.getElementById('gram_barang').value = gram
            //document.getElementById('upd_satuan_barang').value = satuan_barang
            //document.getElementById('upd_stok_barang').value = stok
            document.getElementById('mdl_update_file').value = filename
            document.getElementById('mdl_update_inputtransaksi').value = tgl_input
            const img = document.getElementById("ini");
            img.src = '/static/uploads/'+ filename;
            // document.getElementById("image-holder").innerHTML = "<img src='image.png' alt='The Image' />";
      
        }
        </script>